<% if can? :read, FirstLevel.new(team: current_team) %>
  <%= render 'account/shared/menu/item', {
    url: main_app.polymorphic_path([:account, current_team, :first_levels]),
    label: t('first_levels.navigation.label'),
  } do |p| %>
    <% p.icon do %>
      <i class="<%= t('first_levels.navigation.icon') %>"></i>
    <% end %>
  <% end %>
<% end %>
